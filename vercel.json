{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "index.js"
    },
    {
      "src": "/login",
      "dest": "index.js"
    },
    {
      "src": "/callback",
      "dest": "index.js"
    },
    {
      "src": "/seek",
      "dest": "index.js"
    },
    {
      "src": "/now-playing",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
} 